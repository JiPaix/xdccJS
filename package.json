{
  "name": "xdccjs",
  "version": "4.2.9",
  "description": "download files from XDCC bots on IRC, complete implementation of the XDCC protocol",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/docs",
    "/tests"
  ],
  "bin": {
    "xdccJS": "./dist/bin/index.js"
  },
  "pkg": {
    "scripts": "./dist/*"
  },
  "scripts": {
    "pkg": "pkg ./dist/bin/index.js --out-path executables --compress Brotli",
    "test": "node ./node_modules/mocha/bin/mocha -r ts-node/register tests/mocha.ts",
    "build": "node ./node_modules/rimraf/bin.js dist && node ./build/copyfiles.js && node ./node_modules/typescript/bin/tsc && node ./build/doc.js",
    "prepublishOnly": "npm run build && npm run pkg",
    "snyk-wiz": "node ./node_modules/snyk/dist/cli/index.js wizard",
    "doc": "node ./node_modules/typedoc/bin/typedoc && node ./build/doc.js",
    "intellisense": "node ./build/index.js",
    "postpublish": "node ./build/changelog.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiPaix/xdccJS.git"
  },
  "keywords": [
    "irc",
    "xdcc",
    "ctcp",
    "dcc",
    "pipe",
    "typescript",
    "downloader",
    "download",
    "client",
    "passive",
    "cli"
  ],
  "author": "JiPaix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JiPaix/xdccJS/issues"
  },
  "homepage": "https://github.com/JiPaix/xdccJS#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@wcardinal/typedoc-plugin-named-event": "^1.0.0",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "discord.js": "^12.5.3",
    "dotenv-manipulator": "^3.0.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "form-data": "^4.0.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.0",
    "typedoc-plugin-as-member-of": "^1.0.2",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "commander": "^8.0.0",
    "eventemitter3": "^4.0.0",
    "irc-framework": "^4.7.0",
    "lodash": "^4.17.21",
    "public-ip": "^4.0.2"
  }
}
