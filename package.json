{
  "name": "xdccjs",
  "version": "4.7.0",
  "description": "download files from XDCC bots on IRC, complete implementation of the XDCC protocol",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/tests"
  ],
  "bin": {
    "xdccJS": "./dist/bin/index.js"
  },
  "pkg": {
    "scripts": "./dist/*"
  },
  "scripts": {
    "lint": "eslint \"src/**\"",
    "prebuild": "rimraf dist && node ./node_modules/ts-node/dist/bin.js ./build/copyfiles.ts",
    "build": "node ./node_modules/typescript/bin/tsc ",
    "prepublishOnly": "npm run build",
    "postpublish": "pkg ./dist/bin/index.js --out-path executables --compress Brotli && node ./node_modules/ts-node/dist/bin.js ./build/changelog.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiPaix/xdccJS.git"
  },
  "keywords": [
    "irc",
    "xdcc",
    "ctcp",
    "dcc",
    "pipe",
    "typescript",
    "downloader",
    "download",
    "client",
    "passive",
    "cli"
  ],
  "author": "JiPaix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JiPaix/xdccJS/issues"
  },
  "homepage": "https://github.com/JiPaix/xdccJS#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.192",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chai": "^4.3.7",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "pkg": "^5.8.1",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "commander": "^10.0.0",
    "eventemitter3": "^5.0.0",
    "ip-regex": "^5.0.0",
    "irc-framework": "^4.13.1",
    "lodash": "^4.17.21"
  }
}
