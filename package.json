{
  "name": "xdccjs",
  "version": "2.9.0",
  "description": "download files from XDCC bots on IRC, complete implementation of the XDCC protocol",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/bin",
    "/dist/**",
    "/patches"
  ],
  "bin": {
    "xdccJS": "./bin/index.js"
  },
  "scripts": {
    "test": "snyk test --all-projects && node ./node_modules/mocha/bin/mocha -r ts-node/register test/mocha.ts",
    "build": "node ./node_modules/rimraf/bin.js dist && node ./node_modules/typescript/bin/tsc",
    "prepublishOnly": "npm run build && npm run snyk-wiz",
    "snyk-wiz": "node ./node_modules/snyk/dist/cli/index.js wizard",
    "doc": "node ./node_modules/typedoc/bin/typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiPaix/xdccJS.git"
  },
  "keywords": [
    "irc",
    "xdcc",
    "ctcp",
    "dcc",
    "pipe",
    "typescript",
    "downloader",
    "download",
    "client",
    "passive",
    "cli"
  ],
  "author": "JiPaix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JiPaix/xdccJS/issues"
  },
  "homepage": "https://github.com/JiPaix/xdccJS#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/ip": "^1.1.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.9",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@wcardinal/typedoc-plugin-named-event": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^7.1.1",
    "postinstall-prepare": "^1.0.1",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "snyk": "^1.364.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.6",
    "typedoc-plugin-as-member-of": "^1.0.2",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/colors": "^1.2.1",
    "@types/progress": "^2.0.3",
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "eventemitter3": "^4.0.0",
    "irc-framework": "^4.7.0",
    "progress": "https://github.com/JiPaix/node-progress/tarball/master",
    "public-ip": "^4.0.2"
  },
  "snyk": true
}
